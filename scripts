# Define the sign-up payload
$signupBody = @{
    email = "test2@example.com"
    password = "securepassword2"
} | ConvertTo-Json

# Send the sign-up request
Invoke-WebRequest -Uri http://localhost/signup -Method POST -Body $signupBody -ContentType "application/json"


$signinBody = @{
    email = "test@example.com"
    password = "securepassword"
} | ConvertTo-Json

Invoke-WebRequest -Uri http://localhost/signin -Method POST -Body $signinBody -ContentType "application/json"

$signupBody = @{
    email = "israth@example.com"
    password = "securepassword2"
} | ConvertTo-Json

# Send the sign-up request
Invoke-WebRequest -Uri http://localhost/signup -Method POST -Body $signupBody -ContentType "application/json"
